<h1> Edit Tutoring Info </h1>
<%= form_with(model: @tutor, url: [@course, @tutor], local: true) do |form| %>
  <p>
    <%= form.label :description %><br>
    <%= form.text_area :description %>
  </p>

  <p>
    <%= form.label :rate %><br>
    <%= form.number_field :rate %>
  </p>

  <p>
    <%= form.label :hours %><br>
    <%= form.number_field :hours %>
  </p>

  <p>
    <%= form.label :date %><br>
    <%= form.datetime_select :date %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>